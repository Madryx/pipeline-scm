    {
      steps
      {
        echo 'Built Job-2'
      } 
      }
    stage ('Testing-1')
    {
      steps
      {
        echo 'testing Job-1'
      } 
      }
    stage ('Deploy')
    {
      steps
      {
        echo 'Deploying app-1'
      } 
      }
    
    }
  }
